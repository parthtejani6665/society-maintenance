{
  "expo": {
    "scheme": "digital-dwell",
    "name": "Digital Dwell",
    "slug": "digital-dwell",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.digitaldwell.mobile",
      "config": {
        "googleMapsApiKey": "YOUR_IOS_GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "package": "com.society.ai",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCFhaEamXPrTJzojmUTTJwvfE94ZlK2tH0"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "upi"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "tez"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "paytmmp"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      "@react-native-community/datetimepicker",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "react-native-android-widget",
        {
          "widgets": [
            {
              "name": "MaintenanceSmall",
              "label": "Maintenance (Small)",
              "minWidth": "160dp",
              "minHeight": "160dp",
              "description": "Quick view of maintenance status",
              "previewImage": "./assets/widget-preview.png",
              "resizeMode": "none",
              "targetCellWidth": 2,
              "targetCellHeight": 2
            },
            {
              "name": "MaintenanceMedium",
              "label": "Maintenance (Medium)",
              "minWidth": "320dp",
              "minHeight": "160dp",
              "description": "Maintenance details and action",
              "previewImage": "./assets/widget-preview.png",
              "resizeMode": "horizontal",
              "targetCellWidth": 4,
              "targetCellHeight": 2
            },
            {
              "name": "MaintenanceLarge",
              "label": "Maintenance Dashboard",
              "minWidth": "320dp",
              "minHeight": "320dp",
              "description": "Full maintenance overview",
              "previewImage": "./assets/widget-preview.png",
              "resizeMode": "horizontal|vertical",
              "targetCellWidth": 4,
              "targetCellHeight": 4
            }
          ]
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9fe87013-2145-422f-bc57-230e10b70fe0"
      }
    }
  }
}